---
const LINKS = {
    blog: "/blog",
    contact: "/contact",
    // cv: "/cv/cv.pdf",
    // projects: "#",
    resources: "/resources",
    setup: "/uses",
    friends: "/friends",
    misc: "/miscellaneous",
};
---

<nav>
    <ul>
        {
            Object.entries(LINKS).map(([name, path]) => (
                <li>
                    <a href={path}>{name}</a>
                </li>
            ))
        }
    </ul>
</nav>
