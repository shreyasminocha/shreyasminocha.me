\begin_layout Subsection
Laws
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x}x=1$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x}1=0$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{a}b=\frac{1}{\log_{b}a}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x}a+\log_{x}b=\log_{x}ab$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x}a-\log_{x}b=\log_{x}\frac{a}{b}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $-\log_{x}a=\log_{x}\frac{1}{a}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x}a^{b}=b\log_{x}a$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x^{y}}a=\frac{1}{y}\log_{x}a$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{x^{y}}a^{b}=\frac{b}{y}\log_{x}a$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\log_{b}a=\frac{\log_{x}a}{\log_{x}b}$
\end_inset

 where 
\begin_inset Formula $x$
\end_inset

 is any valid base
