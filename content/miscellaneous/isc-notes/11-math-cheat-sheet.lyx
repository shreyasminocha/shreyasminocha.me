\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{01}=\frac{\sum P_{1}}{\sum P_{0}}\times100
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{01}=\frac{\sum x}{n}
\]

\end_inset

 where 
\begin_inset Formula $x=\frac{\sum P_{1}}{\sum P_{0}}\times100$
\end_inset

.
\end_layout

\begin_layout Subsection
