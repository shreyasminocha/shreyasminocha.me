\begin_layout Standard
Multiply the whole equation by 
\begin_inset Formula $z$
\end_inset

 and solve as a quadratic.
\end_layout

\begin_layout Subsubsection
\begin_inset Formula $(x+a)(x+b)(x+c)(x+d)+k=0$
\end_inset


\end_layout

\begin_layout Standard
When the sum of two of 
\begin_inset Formula $a$
\end_inset

, 
\begin_inset Formula $b$
\end_inset

, 
\begin_inset Formula $c$
\end_inset

, 
\begin_inset Formula $d$
\end_inset

 is equal to the sum of the other two, the equation can be reduced to a
 quadratic.
\end_layout

\begin_layout Standard
Say 
\begin_inset Formula $a+d=b+c=m$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $\left[(x+a)(x+d)\right]\left[(x+b)(x+c)\right]+k=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\left[x^{2}+mx+ad\right]\left[x^{2}+mx+bc\right]+k=0$
\end_inset

.
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $x^{2}+mx$
\end_inset

 be 
\begin_inset Formula $y$
\end_inset

.
\end_layout

\begin_layout Standard
Now solve 
\begin_inset Formula $(y+ad)(y+bc)+k=0$
\end_inset

 as a quadratic.
\end_layout

\begin_layout Subsection
Sums and products of roots
\end_layout

\begin_layout Standard
For a given quadratic equation 
\begin_inset Formula $ax^{2}+bx+c=0$
\end_inset

 with roots 
\begin_inset Formula $\alpha$
\end_inset

 and 
\begin_inset Formula $\beta$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\alpha+\beta=\frac{-b+\sqrt{b^{2}-4ac}}{2a}+\frac{-b-\sqrt{b^{2}-4ac}}{2a}=-\frac{2b}{2a}=-\frac{b}{a}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\alpha\beta=\frac{-b+\sqrt{b^{2}-4ac}}{2a}\times\frac{-b-\sqrt{b^{2}-4ac}}{2a}=\frac{b^{2}-(b^{2}-4ac)}{4a^{2}}=\frac{4ac}{4a^{2}}=\frac{c}{a}
\]

\end_inset


\end_layout

