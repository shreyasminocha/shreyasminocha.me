\begin_layout Section
Compound and Multiple Angles
\end_layout

\begin_layout Subsection
Sum
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin(A+B)=\sin A\cos B+\cos A\sin B
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cos(A+B)=\cos A\cos B-\sin A\sin B
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan(A+B)=\frac{\tan A+\tan B}{1-\tan A\tan B}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cot(A+B)=\frac{\cot A\cot B-1}{\cot A+\cot B}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan(A+B+C)=\frac{\tan A+\tan B+\tan C-\tan A\tan B\tan C}{1-\tan A\tan B-\tan B\tan C-\tan A\tan C}
\]

\end_inset


\end_layout

\begin_layout Subsection
Difference
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin(A-B)=\sin A\cos B-\cos A\sin B
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cos(A-B)=\cos A\cos B+\sin A\sin B
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan(A-B)=\frac{\tan A-\tan B}{1+\tan A\tan B}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cot(A-B)=\frac{\cot A\cot B+1}{\cot A-\cot B}
\]

\end_inset


\end_layout

\begin_layout Subsection
Product to Sum
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\sin A\sin B=\cos(A-B)-\cos(A+B)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\cos A\cos B=\cos(A-B)+\cos(A+B)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\sin A\cos B=\sin(A+B)+\sin(A-B)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\cos A\sin B=\sin(A+B)-\sin(A-B)
\]

\end_inset


\end_layout

\begin_layout Subsection
Sum to Product
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin C+\sin D=2\sin\frac{C+D}{2}\cos\frac{C-D}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
