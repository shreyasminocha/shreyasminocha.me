\uwave off
\noun off
\color none
\begin_inset Formula 
\[
S_{\infty}=\frac{a}{1-r}+\frac{dr}{(1-r)^{2}}
\]

\end_inset

 where 
\begin_inset Formula $a$
\end_inset

, 
\begin_inset Formula $d$
\end_inset

 and 
\begin_inset Formula $r$
\end_inset

 hold the same meanings as above.
\end_layout

\begin_layout Subsection
Series involving natural numbers
\end_layout

\begin_layout Standard
Expressions for the sum of powers of the first 
\begin_inset Formula $n$
\end_inset

 natural numbers can be derived.
\end_layout

\begin_layout Standard
Sum of first 
\begin_inset Formula $n$
\end_inset

 natural numbers is given as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum n=\frac{n(n+1)}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Sum of the squares of the first 
\begin_inset Formula $n$
\end_inset

 natural numbers is given as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum n^{2}=\frac{n(n+1)(2n+1)}{6}
\]

\end_inset


\end_layout

\begin_layout Standard
Sum of the cubes of the first 
\begin_inset Formula $n$
\end_inset

 natural numbers is given as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum n^{3}=\left(\frac{n(n+1)}{2}\right)^{2}=\left(\sum n\right)^{2}
\]

\end_inset


\end_layout

