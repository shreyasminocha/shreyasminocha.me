\begin_layout Standard
\begin_inset Formula 
\[
\csc^{-1}x=\sin^{-1}\frac{1}{x}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sec^{-1}x=\cos^{-1}\frac{1}{x}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cot^{-1}x=\begin{cases}
\tan^{-1}\frac{1}{x} & x>0\\
\pi+\tan^{-1}\frac{1}{x} & x<0
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin^{-1}(-x)=-\sin^{-1}x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\csc^{-1}(-x)=-\csc^{-1}x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan^{-1}(-x)=-\tan^{-1}x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cos^{-1}(-x)=\pi-\cos x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sec^{-1}(-x)=\pi-\sec x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cot^{-1}(-x)=\pi-\cot x
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin^{-1}x+\cos^{-1}x=\frac{\pi}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan^{-1}x+\cot^{-1}x=\frac{\pi}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\csc^{-1}x+\sec^{-1}x=\frac{\pi}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $xy<1$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan^{-1}x+\tan^{-1}y=\tan^{-1}\frac{x+y}{1-xy}
\]

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $xy>-1$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tan^{-1}x-\tan^{-1}y=\tan^{-1}\frac{x-y}{1+xy}
\]

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $x^{2}<1$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\tan^{-1}x=\tan^{-1}\frac{2x}{1-x^{2}}=\sin^{-1}\frac{2x}{1+x^{2}}=\cos^{-1}\frac{1-x^{2}}{1+x^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\sin^{-1}x=\sin^{-1}\left(2x\sqrt{1-x^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2\cos^{-1}x=\cos^{-1}(2x^{2}-1)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
3\sin^{-1}x=\sin^{-1}(3x-4x^{3})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
3\cos^{-1}x=\cos^{-1}(4x^{3}-3x)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
3\tan^{-1}x=\tan^{-1}\frac{3x-x^{3}}{1-3x^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
If <conditions>,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin^{-1}x+\sin^{-1}y=\sin^{-1}\left(x\sqrt{1-y^{2}}+y\sqrt{1-x^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
If <conditions>,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sin^{-1}x-\sin^{-1}y=\sin^{-1}\left(x\sqrt{1-y^{2}}-y\sqrt{1-x^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\cos^{-1}x+\cos^{-1}y=\cos^{-1}\left(xy-\sqrt{1-x^{2}}\cdot\sqrt{1-y^{2}}\right)
\]

\end_inset


\end_layout

