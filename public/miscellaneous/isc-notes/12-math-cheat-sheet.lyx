\begin_layout Subsection
Adjoint
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathrm{adj}\begin{bmatrix}a & b\\
c & d
\end{bmatrix}=\begin{bmatrix}d & -b\\
-c & a
\end{bmatrix}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathrm{adj}\begin{bmatrix}a & b & c\\
d & e & f\\
g & h & i
\end{bmatrix}=\begin{bmatrix}+\begin{vmatrix}e & f\\
h & i
\end{vmatrix} & -\begin{vmatrix}d & f\\
g & i
\end{vmatrix} & +\begin{vmatrix}d & e\\
g & h
\end{vmatrix}\\
-\begin{vmatrix}b & c\\
h & i
\end{vmatrix} & +\begin{vmatrix}a & c\\
g & i
\end{vmatrix} & -\begin{vmatrix}a & b\\
g & h
\end{vmatrix}\\
+\begin{vmatrix}b & c\\
e & f
\end{vmatrix} & -\begin{vmatrix}a & c\\
d & f
\end{vmatrix} & +\begin{vmatrix}a & b\\
d & e
\end{vmatrix}
\end{bmatrix}^{'}
\]

\end_inset


\end_layout

